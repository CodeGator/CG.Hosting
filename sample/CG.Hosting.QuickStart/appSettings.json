{
  "A": "value from the appsettings.json",
  "Alerts": {
    "InformationAlerts": {},
    "WarningAlerts": {},
    "ErrorAlerts": {
      "Email": {
        "Enabled": true,
        "To": "support@wherever.com",
        "From": "mysample@wherever.com",
        "Subject": "%APP% Error!",
        "Body": "An error was encountered: %MSG%",
        "IsHtml": false
      }
    },
    "CriticalAlerts": {
      "Email": {
        "Enabled": true,
        "To": "support@wherever.com",
        "From": "mysample@wherever.com",
        "Subject": "%APP% Critical Error!",
        "Body": "A <b>critical</b> error was encountered and the application is shutting down.<br />The error was: %MSG%",
        "IsHtml":  true
      }
    },
    "AuditAlerts": {},
    "DebugAlerts": {},
    "TraceAlerts": {}
  },
  "Services": {
    "Email": {
      "Strategy": {
        "Name": "Smtp",
        "Assembly":  ""
      },
      "Smtp": {
        "ServerAddress": "localhost",
        "ServerPort": 25,
        "UserName": "test@blah.com",
        "Password": ""
      }
    },
    "Logging": {
      "Strategy": {
        "Name": "Serilog",
        "Assembly": ""
      },
      "Serilog": {
        "WriteTo": [ "Console" ]
      }
    }
  }
